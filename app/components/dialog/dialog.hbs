<div class="dialog {{class}}" id="{{id}}"><div class="dialog-top">{{#if title}}<span class="title"><b> {{title}} </b></span>{{/if}}</div><div class="dialog-body">{{#if message}}<span class="message"> {{message}} {{#if link}} <a href="{{{href}}}">{{link}}</a> {{/if}}</span>{{/if}}</div><div class="dialog-actions">{{#if button}}{{#each button}}<span class="dialog-button pull-right">{{>PartialButton}}</span>{{/each}}{{/if}}</div></div>